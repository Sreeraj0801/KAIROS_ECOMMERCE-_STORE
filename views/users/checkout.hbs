<script>
    $("#checkoutForm").submit((e) => {
        e.preventDefault()
        $.ajax({
            url: '/placeOrder',
            method: 'post',
            data: $('#checkoutForm').serialize(),
            success: (response) => {
                if (response.paymentMethod == 'COD') {
                Swal.fire({
                title: 'Ordered succesfully!',
                text: 'Your Order has Placed.',
                imageUrl: 'https://i.pinimg.com/originals/b9/88/b7/b988b7c3e84e1f83ef9447157831b460.gif',
                imageWidth: 400,
                imageHeight: 200,
                imageAlt: 'Custom image',
                }).then(()=>{
                 location.href = '/profile'
                })
                }
                else if (response.paymentMethod == 'Razorpay') {
                    razorpayPayment(response)
                }
                else if(response.walletPayment)
                {
                Swal.fire({
                title: 'Ordered succesfully!',
                text: 'Your Order has Placed.',
                imageUrl: 'https://i.pinimg.com/originals/0d/e4/1a/0de41a3c5953fba1755ebd416ec109dd.gif',
                imageWidth: 400,
                imageHeight: 200,
                imageAlt: 'Custom image',
                }).then(()=>{
                 location.href = '/profile'
                })
                }
                else {
                    location.href = response;
                }
            }
        })
    })


    function razorpayPayment(order) {
        var options = {
            "key": "rzp_test_jHvJ8dxfPD9dqC", // Enter the Key ID generated from the Dashboard
            "amount": order.amount, // Amount is in currency subunits. Default currency is INR. Hence, 50000 refers to 50000 paise
            "currency": "INR",
            "name": "KAIROS",
            "description": "Test Transaction",
            "image": "https://example.com/your_logo",
            "order_id": order.id, //This is a sample Order ID. Pass the `id` obtained in the response of Step 1
            "handler": function (response) {
                verifyPayment(response, order)
            },
            "prefill": {
                "name": order.name,
                "email": "gaurav.kumar@example.com",
                "contact": "9999999999"
            },
            "notes": {
                "address": "Razorpay Corporate Office"
            },
            "theme": {
                "color": "#3399cc"
            }
        };
        var rzp1 = new Razorpay(options);
        rzp1.open();
    }

    function verifyPayment(payment, order) {
        $.ajax({
            url: '/verifyPayment',
            data: {
                payment,
                order
            },
            method: 'post',
            success: (response) => {
                if (response.status) {
                Swal.fire({
                title: 'Ordered succesfully!',
                text: 'Your Order has Placed.',
                imageUrl: 'https://i.pinimg.com/originals/0d/e4/1a/0de41a3c5953fba1755ebd416ec109dd.gif',
                imageWidth: 400,
                imageHeight: 200,
                imageAlt: 'Custom image',
                }).then(()=>{
                 location.href = '/profile'
                })
                }
                else {
                    alert('payment failed')
                }
            }
        })
    }
</script>
